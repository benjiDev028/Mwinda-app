{
  "expo": {
    "name": "mwinda",
    "slug": "mwinda",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "userInterfaceStyle": "light",
    "backgroundColor": "#fec107",
    "newArchEnabled": true,
    "permissions": ["CAMERA"],
    
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourapp.identifier",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app requires microphone access"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#fec107",
        "permissions": ["CAMERA"]
        
      }
    },
    "web": {
      "favicon": "./assets/splash-logo.jpg"
    },
    "plugins": [
      "expo-localization",
      

      "expo-barcode-scanner"
    ]
  }
}
